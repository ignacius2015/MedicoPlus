<Window
    x:Class="MedicoPlus.Helpers.Admin.RightSide.user"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:MedicoPlus.Helpers.Admin.RightSide"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:regions="http://prismlibrary.com/"
    Title="Налаштування користувачів"
    Width="800"
    Height="600">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="3*" />
            <ColumnDefinition Width="7*" />
        </Grid.ColumnDefinitions>
        <ItemsControl Grid.Column="0" regions:RegionManager.RegionName="LeftContent" />
        <ItemsControl Grid.Column="1" regions:RegionManager.RegionName="RightContent" />
    </Grid>
</Window>
